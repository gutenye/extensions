{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quick-image",
  "title": "Quick Image",
  "description": "Quickly Convert and Concatenate Images",
  "categories": [
    "Media",
    "Productivity",
    "Design Tools"
  ],
  "icon": "command-icon.png",
  "author": "gutenye",
  "license": "MIT",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "test": "jest",
    "publish": "ray publish"
  },
  "commands": [
    {
      "name": "quick-image",
      "title": "Quick Image",
      "description": "Convert and Concatenate Images",
      "mode": "no-view",
      "arguments": [
        {
          "name": "input",
          "placeholder": "Command",
          "type": "text"
        }
      ]
    },
    {
      "name": "help",
      "title": "Help",
      "description": "Display Help",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@gutenye/sharp": "^0.33.5-patch1",
    "@raycast/api": "^1.88.4",
    "lodash": "^4.17.21",
    "node-addon-api": "^7.1.0",
    "node-gyp": "^10.0.1",
    "pdfmake": "^0.3.0-beta.15",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.13",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
